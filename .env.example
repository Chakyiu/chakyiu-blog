# Copy this file to .env and fill in the values before running the application.

# Database connection string (PostgreSQL).
DATABASE_URL=postgres://postgres:postgres@localhost:5432/blog

# Secret key for authentication. Use a strong, random string in production.
AUTH_SECRET=your-secret-here

# OAuth credentials for GitHub authentication. Replace with your own values.
AUTH_GITHUB_ID=your-github-client-id
AUTH_GITHUB_SECRET=your-github-client-secret

# Base URL for the application. Update if running on a different host or port.
AUTH_URL=http://localhost:3000
DATA_DIR=./data

# Google Search Console verification token (get from https://search.google.com/search-console)
NEXT_PUBLIC_GSC_VERIFICATION=
